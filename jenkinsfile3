node {
    stage ('git scm') {
        git branch: 'main', url: 'https://github.com/YOGESHTC/maven-project.git'
    }
    stage ('build') {
        sh 'mvn install'
    }
    stage ('test') {
        echo "this is code test stage"
    }
    stage ('deploy') {
        echo "this is code deploy stage"
    }
}
